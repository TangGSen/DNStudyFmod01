<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/typeNormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="typeNormal"
        android:text="原声" />

    <Button
        android:layout_below="@+id/typeNormal"
        android:id="@+id/typeLuoLi"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="typeLuoLi"
        android:text="萝莉" />

    <Button
        android:layout_below="@+id/typeLuoLi"
        android:id="@+id/typeDaShu"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="typeDaShu"
        android:text="大叔" />

</RelativeLayout>
